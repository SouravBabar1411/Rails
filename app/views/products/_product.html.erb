<div id="<%= dom_id product %>">
  <p>
    <strong>Productname:</strong>
    <%= product.productname %>
  </p>

  <p>
    <strong>Quantity:</strong>
    <%= product.quantity %>
  </p>

  <p>
    <strong>Brand:</strong>
    <%= product.brand %>
  </p>

  <p>
    <strong>Prize:</strong>
    <%= product.prize %>
  </p>

  <p>
    <strong>Mandate:</strong>
    <%= product.mandate %>
  </p>

  <p>
    <strong>Expdate:</strong>
    <%= product.expdate %>
  </p>

</div>
