<p id="notice"><%= notice %></p>

<h1>Users</h1>

<table class="table table-bordered" id='user'>
  <thead class="table-dark">
    <tr>
      <th>Image</th>
      <th>first_name</th>
      <th>last_name</th>
      <th>email</th>
      <th>phone</th>
      <th>gender</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><img src="<%= user.image %>"></td>
        <td><%= user.first_name%></td>
        <td><%= user.last_name %></td>
        <td><%= user.email%></td>
        <td><%= user.phone %></td>
        <td><%= user.gender %></td>
      </tr>
    <% end %>
  </tbody>
</table>